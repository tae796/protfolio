    $(function() {

    	function getToday(){
    	    var date = new Date();
    	    var year = date.getFullYear();
    	    var month = ("0" + (1 + date.getMonth())).slice(-2);
    	    var day = ("0" + date.getDate()).slice(-2);

    	    return year + "-" + month + "-" + day;
    	}
    	function getToday_1(){
    	    var now = new Date();
    	    var date = new Date(now.setDate(now.getDate() - 1));
    	    var year = date.getFullYear();
    	    var month = ("0" + (1 + date.getMonth())).slice(-2);
    	    var day = ("0" + date.getDate()).slice(-2);

    	    return year + "-" + month + "-" + day;
    	}
    	function getToday_2(){
    	    var now = new Date();
    	    var date = new Date(now.setDate(now.getDate() - 2));
    	    var year = date.getFullYear();
    	    var month = ("0" + (1 + date.getMonth())).slice(-2);
    	    var day = ("0" + date.getDate()).slice(-2);

    	    return year + "-" + month + "-" + day;
    	}
    	
    	var now = getToday();
    	var yester = getToday_1();
